  "bundleDependencies": false,
  "deprecated": false,
  "description": "A base set of recommended ESLint rules for TypeScript projects",
  "devDependencies": {
    "@types/node": "^10.12.21",
    "@typescript-eslint/eslint-plugin": "^1.2.0",
    "@typescript-eslint/parser": "^1.2.0",
    "create-react-class": "^15.6.3",
